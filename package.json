{
  "name": "bootstrap",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo e-commerce Althea Systems (Next.js, NestJS, PostgreSQL, MongoDB)",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
